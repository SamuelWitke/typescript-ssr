{
  "name": "server-side-typescript",
  "version": "1.0.0",
  "description": "Server Side TypeScript React ",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "tsc && webpack --progress",
    "build:server": "tsc --lib dom,es2016 --jsx react src/server/index.tsx ",
    "start:server": "node ./src/server/index.js",
    "clean": "rd /s/q built",
    "prepare": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "author": "Samuel Witke",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/history": "^4.6.2",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.3.4",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "4.0.26",
    "@types/react-router-config": "1.0.7",
    "@types/react-router-dom": "4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-form": "^7.4.3",
    "@types/redux-logger": "^3.0.6",
    "@types/serialize-javascript": "^1.3.2",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.3.0",
    "semantic-ui-react": "^0.82.0",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.21.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.10",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "23.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.1.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "semantic-ui-css": "^2.3.3",
    "source-map-loader": "0.2.3",
    "ts-import-plugin": "^1.5.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-livereload-plugin": "^2.1.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
